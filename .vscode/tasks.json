{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "func",
      "command": "host start",
      "problemMatcher": "$func-watch",
      "isBackground": true,
      "dependsOn": "yarn build",
      "options": {
        "cwd": "${workspaceFolder}/apiv2"
      }
    },
    {
      "type": "shell",
      "label": "yarn build",
      "command": "yarn run build",
      "dependsOn": "yarn",
      "problemMatcher": "$tsc",
      "options": {
        "cwd": "${workspaceFolder}/apiv2"
      }
    },
    {
      "type": "shell",
      "label": "yarn",
      "command": "yarn",
      "options": {
        "cwd": "${workspaceFolder}/apiv2"
      }
    }
  ]
}
